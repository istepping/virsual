    <add key="BankOrderPath" value=".\private$\bankorder"></add>
    <!--网银订单结果异步通知队列-->
    <add key="BankNotifyPath" value=".\private$\banknotify"></add>
    <!--卡类订单队列-->
    <add key="CardOrderPath" value=".\private$\cardorder"></add>
    <!--卡类订单结果异步通知队列-->
