    <!--短信订单结果异步通知队列-->
    <add key="SmsNotifyPath" value=".\private$\smsnotify"></add>
    <!--实现MSMQ对队的程序集-->
    <add key="OrderMessaging" value="KuaiCard.MSMQMessaging"></add>
